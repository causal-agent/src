# $FreeBSD: releng/12.0/bin/sh/tests/builtins/wait1.0 208476 2010-05-23 22:10:20Z jilles $

failures=
failure() {
	echo "Error at line $1" >&2
	failures=x$failures
}

exit 4 & p4=$!
exit 8 & p8=$!
wait $p4
[ $? = 4 ] || failure $LINENO
wait $p8
[ $? = 8 ] || failure $LINENO

exit 3 & p3=$!
exit 7 & p7=$!
wait $p7
[ $? = 7 ] || failure $LINENO
wait $p3
[ $? = 3 ] || failure $LINENO

test -z "$failures"
